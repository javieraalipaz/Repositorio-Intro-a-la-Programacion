#include "stdafx.h"
#include<iostream>
#include "conio.h"

using namespace std;

void main ()
{
    float A, B, L;
		cout << "Ingrese base del rectangulo: ";
		cin >> B;

		cout << "Ingrese el largo del rectangulo: ";
		cin >> L;

		A= B*L;

		cout << "El area del rectangulo es: "<<A<<endl;

		getch();

}
